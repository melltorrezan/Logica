Algoritmo "semnome"

Var

   t, n1, n2: inteiro

Inicio

      escreval("Digite um número: ")
      leia(n1)
      
      escreval("Digite outro número: ")
      leia(n2)
      
      t <- n1
      
      n1 <- n2
      
      n2 <- t
      
      escreval("O novo valor do primeiro número é: ", n1)
      escreval("O novo valor do segundo número é: ", n2)

Fimalgoritmo